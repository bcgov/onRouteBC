{
  "name": "onroutebc-sftp-api",
  "version": "0.0.1",
  "description": "OnRouteBC Document Operations (SFTP) api.",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:staged": "./node_modules/.bin/lint-staged",
    "make-badges": "istanbul-badges-readme --logo=jest --exitCode=1",
    "make-badges:ci": "npm run make-badges -- --ci",
    "test": "jest",
    "test:clear_jest": "jest --clearCache",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/e2e/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bcgov/onroutebc.git"
  },
  "keywords": [
    "openshift",
    "pipeline",
    "node",
    "node",
    "js",
    "devops"
  ],
  "author": "Derek Roberts",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bcgov/onroutebc/issues"
  },
  "homepage": "https://github.com/bcgov/onroutebc#readme",
  "dependencies": {
    "@automapper/classes": "^8.7.7",
    "@automapper/core": "^8.7.7",
    "@automapper/nestjs": "^8.7.7",
    "@automapper/types": "^6.3.1",
    "@aws-sdk/client-s3": "^3.369.0",
    "@aws-sdk/lib-storage": "^3.370.0",
    "@aws-sdk/s3-request-presigner": "^3.370.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.0.1",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.4.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/testing": "^9.4.3",
    "@nestjs/typeorm": "^10.0.0",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.1.4",
    "jwks-rsa": "^3.0.1",
    "mssql": "^9.1.1",
    "nest-sftp": "^2.0.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pdf-lib": "^1.17.1",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "^0.3.16",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.17.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "istanbul-badges-readme": "^1.8.5",
    "jest": "^29.5.0",
    "jest-badges": "^0.1.3",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1",
      "^test/(.*)$": "<rootDir>/test/$1"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      ".module.ts",
      "<rootDir>/src/main.ts"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "cobertura",
      "jest-badges"
    ],
    "testEnvironment": "node",
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "lint-staged": {
    "*.{js,ts}": "./node_modules/.bin/eslint --cache --fix"
  },
  "overrides": {
    "minimist@<1.2.6": "1.2.6"
  }
}
